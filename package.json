{
  "name": "m31-internet-shop",
  "version": "0.0.0",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "start:dev": "cd start && npm run start:dev",
    "build:eshop": "cd e-shop && ng build --prod",
    "build:admin": "cd admin && ng build --prod",
    "build:server": "cd server && npm run build",
    "build": "npm run build:eshop && npm run build:admin && npm run build:server",
    "serve": "npm run build:server && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "8"
  },
  "main": "dist/server/main.js",
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.0.2",
    "body-parser": "^1.19.0",
    "firebase": "^6.1.0",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^2.3.0",
    "grpc": "^1.21.1",
    "guid-typescript": "^1.0.9",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.5"
  },
  "devDependencies": {
    "@firebase/firestore-types": "^1.3.0",
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "firebase-functions-test": "^0.1.6",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "private": true
}
