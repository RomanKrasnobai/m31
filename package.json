{
  "name": "m31-internet-shop",
  "version": "0.0.0",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "start:dev": "cd start && npm run start:dev",
    "build:eshop": "cd e-shop && ng build --prod",
    "build:admin": "cd admin && ng build --prod",
    "build:server": "cd server && npm run build",
    "build": "npm run build:eshop && npm run build:admin && npm run build:server",
    "serve": "npm run build:server && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "8"
  },
  "main": "dist/server/main.js",
  "dependencies": {
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^2.3.0",
    "grpc": "^1.21.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
